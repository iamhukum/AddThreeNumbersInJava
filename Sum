import java.util.Scanner;

public class Practice {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("Enter three numbers");
        int n1, n2, n3;
        n1 = s.nextInt();
        n2 = s.nextInt();
        n3 = s.nextInt();
        int sum = n1 + n2 + n3;
        System.out.println(sum);
    }

}
